{
    "requires": [
        "bare-metal",
        "rtos",
        "cmsis-cmsis5-rtos2"
    ],
    "config": {
        "main_stack_size": {
            "value": 1024
        }
    },
    "target_overrides": {
        "*": {
            "target.c_lib": "small",
            "target.printf_lib": "minimal-printf",
            "platform.minimal-printf-enable-floating-point": true,
            "platform.stdio-minimal-console-only": true,
            "platform.stdio-baud-rate": 115200,
            "platform.default-serial-baud-rate": 115200
        }
    }
}
