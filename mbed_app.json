{
    "requires": [
        "bare-metal",
        "rtos",
        "cmsis-cmsis5-rtos2",
        "sd",
        "filesystem",
        "fat_chan",
        "littlefs2"
    ],
    "config": {
        "main_stack_size": {
            "value": 2048
        }
    },
    "target_overrides": {
        "*": {
            "platform.stdio-baud-rate": 57600,
            "platform.default-serial-baud-rate": 57600,
            "platform.minimal-printf-enable-floating-point": true,
            "sd.SPI_MOSI": "NC",
            "sd.SPI_MISO": "NC",
            "sd.SPI_CLK": "NC",
            "sd.SPI_CS": "NC",
            "target.components_add": [
                "SD"
            ]
        }
    }
}
